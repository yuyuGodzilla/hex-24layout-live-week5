<div class="modal fade" id="editBackdrop" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1"
  aria-labelledby="editBackdropLabel" aria-hidden="true">
  <div class="modal-dialog modal-dialog-scrollable">
    <div class="modal-content">
      <div class="modal-header pt-5 px-5 border-bottom-0">
        <h5 class="modal-title" id="editBackdropLabel">Edit Admin</h5>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body px-5">
        <form>
          <fieldset class="border-bottom border-gray-400">
            <legend class="fs-5 fw-bold mb-3">Account</legend>
            <div class="mb-3">
              <label for="editInputName1" class="form-label mb-0 fs-7 text-gray-900">Name</label>
              <input value="Emir Wicks" readonly type="name" class="form-control border-gray-900 bg-gray-100 p-3"
                id="editInputName1" aria-describedby="nameHelp">
            </div>
            <div class="mb-4">
              <label for="editInputEmail1" class="form-label mb-0 fs-7 text-gray-900">Email</label>
              <input value="emir.wicks@mail.com" readonly type="email"
                class="form-control border-gray-900 bg-gray-100 p-3" id="editInputEmail1">
            </div>
          </fieldset>
          <fieldset class="py-4 border-bottom border-gray-400">
            <legend class="fs-5 fw-bold mb-3">Verification</legend>
            <div class="mb-3 d-flex justify-content-between align-items-center">
              <div class="fw-bold"><span class="material-symbols-outlined align-bottom text-success me-2">
                  check_circle
                </span>Verified
              </div>
              <time datetime="2020-4-12" class="fs-8 text-gray-900">2020/4/12</time>

            </div>
            <button disabled type="button" class="btn btn-outline-secondary w-100 py-3">Send Verification Mail</button>
          </fieldset>
          <fieldset class="py-4 border-bottom border-gray-400">
            <legend class="fs-5 fw-bold mb-3">Access Level</legend>
            <select class="form-select text-gray-900 border-gray-900" aria-label="Default select example">
              <option disabled>-Choose access level-</option>
              <option selected value="admin">Admin</option>
              <option value="editor">Editor</option>
              <option value="viewer">Viewer</option>
            </select>
          </fieldset>
          <fieldset class="py-4 border-bottom border-gray-400">
            <legend class="fs-5 fw-bold mb-3">Custom Access</legend>
            <div class="form-check mb-3">
              <input class="form-check-input me-2" type="checkbox" value="" id="editCheckLecture">
              <label class="form-check-label" for="editCheckLecture">
                Lecture
              </label>
            </div>
            <div class="form-check mb-3">
              <input class="form-check-input me-2" type="checkbox" value="" id="editCheckCoupon">
              <label class="form-check-label" for="editCheckCoupon">
                Coupon
              </label>
            </div>
            <div class="form-check mb-3">
              <input class="form-check-input me-2" type="checkbox" value="" id="editCheckMessage">
              <label class="form-check-label" for="editCheckMessage">
                Message
              </label>
            </div>
            <div class="form-check mb-3">
              <input class="form-check-input me-2" type="checkbox" value="" id="editCheckSetting">
              <label class="form-check-label" for="editCheckSetting">
                Setting
              </label>
            </div>
            <div class="form-check mb-3">
              <input class="form-check-input me-2" type="checkbox" value="" id="editCheckTask">
              <label class="form-check-label" for="editCheckTask">
                Task Menu
              </label>
            </div>
          </fieldset>
          <fieldset class="py-4 ">
            <legend class="fs-5 fw-bold mb-3">Verification</legend>
            <div class="mb-3 fw-bold text-danger"><span class="material-symbols-outlined align-bottom text-danger me-2">
              warning
              </span>This cannot be undone!</div>
            <button type="button" class="btn btn-outline-danger fw-bold w-100 py-3">Deactivate This Account</button>
          </fieldset>
        </form>
      </div>
      <div class="modal-footer px-5 py-3 border-top-0">
        <button type="button" class="btn btn-outline-success py-2 px-5 fw-bold border-gray-400"
          data-bs-dismiss="modal">Cancel</button>
        <button type="button" class="btn btn-success py-2 px-5 fw-bold" data-bs-dismiss="modal">Update</button>
      </div>
    </div>
  </div>
</div>